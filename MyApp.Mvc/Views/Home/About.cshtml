@model MyApp.Mvc.ViewModels.AboutViewModel
@{
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<div class="container">
    <div class="text">@Model.Address</div>
    <div>
        @foreach (var S in Model.Suppliers)
        {
            <h2 src=@("https://localhost:44389/api/suppliers")>@S.SupplierName</h2>
        }
    </div>
</div>
@if (User.IsInRole("Admin"))
{
    <li><a asp-area="" asp-controller="Home" asp-action="Admin">Admin</a></li>
}
